{
    "editor.autoClosingBrackets": false, 
    "editor.cursorStyle": "block",
    "editor.fontSize": 12,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.scrollBeyondLastLine": false,
    "editor.tabSize": 2,
    "files.eol": "\n",
